<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/alert_dialog_bottom_bg"
    android:orientation="vertical">
    <TextView
        android:id="@+id/mm_member_unit"
        style="@style/NoInsuranceMoneyDialogButtonStyle"
        android:text="联系会员管理单位"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/divider"/>
    <TextView
        android:id="@+id/introduce_deposit_card"
        style="@style/NoInsuranceMoneyDialogButtonStyle"
        android:text="@string/hl_deposit_card_introduce"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/divider"/>
    <TextView
        android:id="@+id/cancel"
        style="@style/NoInsuranceMoneyDialogButtonStyle"
        android:text="@string/cancel"/>
</LinearLayout>